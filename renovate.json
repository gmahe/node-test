{
  "assignees": ["gmahe"],
  "prTitle": "Vroom {{depName}} + prTitle",
  "commitMessage": "Vroom vroom dependency {{depName}} to version {{newValue}} + commitMessage",
  "packageRules": [
    {
      "packagePatterns": ["^eslint"],
      "groupName": "eslint packages"
    }
  ],
  "repositories": [
    {
      "packageRules": [
        {
          "paths": ["dir1/package.json"],
          "labels": ["dir1", "Renovate"]
        }
      ]
    },
    {
      "packageRules": [
        {
          "paths": ["dir2/package.json"],
          "labels": ["dir2", "Renovate"]
        }
      ]
    }
  ]
}
